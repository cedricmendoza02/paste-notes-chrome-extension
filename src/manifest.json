{
    "name": "Paste Notes",
    "description": "Paste pre-defined notes to input field",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["https://cbre.service-now.com/*"],
        "js": ["content-script.js"]
      }
    ],
    "options_page": "options.html", 
    "permissions": [
      "storage",
      "contextMenus",
      "activeTab"
    ]
  }